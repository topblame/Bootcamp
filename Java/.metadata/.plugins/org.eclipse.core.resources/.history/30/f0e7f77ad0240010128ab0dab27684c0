package chapter15.hashmap;

import java.util.HashMap;
import java.util.Map;

public class HashMapMain_01 {
	public static void main(String[] args) {
		Map<String, Integer> map = new HashMap<String, Integer>();
		map.put("김성규", 95);
		map.put("김민혁", 85);
		map.put("임지아", 90);
		map.put("유성연", 100);
	}
}
