package chapter15.treemap;

import java.util.Map;
import java.util.Map.Entry;
import java.util.TreeMap;

public class TreeMapEmployeeMain_02 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		TreeMap<Employee, Integer> employeeMap = new TreeMap<Employee, Integer>();
		
		//employeemap에 5개 추가
		employeeMap.put(new Employee("David", 20), 1);
		employeeMap.put(new Employee("Charlie", 20), 2);
		employeeMap.put(new Employee("ALICE", 20), 3);
		employeeMap.put(new Employee("KON", 20), 4);
		employeeMap.put(new Employee("Bob", 20), 5);
		
		System.out.println();
		
		for(Entry<Employee, Integer> entry : employeeMap.entrySet()) {
			System.out.println(entry.getKey() + ":" + entry.getValue());
		}
		
		//출력
		Integer employeeId = 1;
		System.out.println("\n "+ employeeName + "는 " + employeeMap.get(employeeName));
		//특정 직원 출력
	}

}
