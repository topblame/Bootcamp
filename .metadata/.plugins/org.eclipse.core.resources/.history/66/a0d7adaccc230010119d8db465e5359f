package chapter14;

import java.util.ArrayList;

public class ArrayList_02 {
	public static void main(String[] args) {
		ArrayList<Integer> list = new ArrayList<Integer>();
		System.out.println("list.size: " + list.size());
		
		int[] arr = {1, 2, 3 ,4 ,5};
		System.out.println(arr.length);
		
		list.add(100);
		list.add(50);
		System.out.println("list.size: " + list.size());
	}
}
