package chapter08;

public class SportCar extends Car{
	
	@Override
	public void speedUp() {
		speed += 10;
		System.out.println("speed  : "  + speed);
	}

	@Override
	public void stop() {
		// TODO Auto-generated method stub
		System.out.println("스포츠카가 멈춥니다.");
		speed = 0;
	}

	public static void main(String[] args) {
		SportCar sc = new SportCar()	;
		sc.speedUp();
		sc.stop();
	}
}
