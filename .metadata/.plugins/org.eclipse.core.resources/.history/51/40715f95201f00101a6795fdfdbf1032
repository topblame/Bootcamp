package chapter08;

public class Samsung extends Appliance{
	public int elecNum;
	public Samsung(String brand) {
		// TODO Auto-generated constructor stub
		super(brand);
	}

	@Override
	void turnOn() {
		// TODO Auto-generated method stub
		super.turnOn();
		System.out.println("삼성 세탁기를 작동시킵니다. ");
	}

	@Override
	void turnOff() {
		// TODO Auto-generated method stub
		super.turnOff();
	}
	
}
