package chapter12.clone;

public class ArrayCloneMain_01 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[] arr1 = {1, 2, 3, 4, 5};
		int[] arr2;
		arr2 = arr1.clone();
		System.out.println("복사된 arr2 배열");
		for(int num: arr2) {
			System.out.println(num + " ");
		}
	}

}
