package chapter11;

public class StaticInner_03 {
	int a = 10;
	private int b = 100; // class 내부에서 사용가능
	static int c = 200; // Data 영역
	
	//생성자 
	//메서드
	//내부 클래스 (Data 영역)
	static class Inner{
	
		//필드
		static int d = 1000;
		//생성자
		
		//메서드
		public void printData() {
			System.out.println("int a : " + d);
		}//printData
	}//Inner Class
	
	//-------------------------------------------------------------------------------------
	public static void main(String[] args) {
		Inner inner = new Inner();
		
		
	}//main
	
	
}//class
