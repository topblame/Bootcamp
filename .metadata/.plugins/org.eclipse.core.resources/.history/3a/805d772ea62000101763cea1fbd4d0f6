package chapter11;

class Out{
	
	//멤버변수 
	static int a =1;
	
	//생성자 default
	
	//메서드
	
	//내부 클래스 
	public class In{
		//멤버변수
		
		//생성자
		
		//메서드
		public String Infun() {
			return a+"번째 Non-Static 내부";
		}
		
	}//In
	
}//Out

public class NonStatic_01 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		// #1 Out Class 객체.
		Out obj1 = new Out();
		// #2 In  class 객체.
		Out.In obj2 = obj1.new In();
		String str = obj2.Infun();
	}

}
