package chapter13;

public class GeneralNoGeneric {
	public void printIntArr(Integer[] arr) {
		for (int i = 0; i < arr.length; i++) {
			System.out.println(" "+ arr[i]);
		}
		System.out.println();
	}
	public void printDoubleArr(Double[] arr) {
		for (int i = 0; i < arr.length; i++) {
			System.out.println(" "+ arr[i]);
		}
		System.out.println();
	}
	//문자배열 출력 메서드
	public void printCharArr(Character[] arr) {
		for (int i = 0; i < arr.length; i++) {
			System.out.println(" "+ arr[i]);
		}
		System.out.println();
	}
}
