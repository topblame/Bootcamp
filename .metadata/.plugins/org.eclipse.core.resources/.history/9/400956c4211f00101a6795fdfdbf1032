package chapter08;

public class Appliance {
	private String brand;
	
	// 생성자로 초기화
	public Appliance() {
		// TODO Auto-generated constructor stub
	}
	public Appliance(String brand) {
		this.brand = brand;
	}
	
	void turnOn() {
		System.out.println("brand" + " 전원을 켭니다.");
	}

	void turnOff() {
		System.out.println("brand" + " 전원을 끕니다.");
	}
}
