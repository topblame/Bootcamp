package chapter14.treeset;

import java.util.Set;
import java.util.TreeSet;

public class TreeSetMain_01 {
	public static void main(String[] args) {
		Set<String> treeSet = new TreeSet<String>();
		//요소추가
		
		TreeSet.add("Apple");
		TreeSet.add("Orange");
		TreeSet.add("Banana");
		
		TreeSet.add("Apple");
		System.out.println("HashSet : " + TreeSet);
		
		if (TreeSet.contains("Banana")) {
			System.out.println("Banana is in the Hashset");
		}
		//요소 삭제
		TreeSet.remove("orange");
		System.out.println("After Remove "+TreeSet);
	}
}
